    "node_vertex_A" -> "ch_inbox_B" [label="to_B", color=green];
    "node_vertex_A" -> "ch_inbox_C" [label="to_C", color=green];
    "node_vertex_B" -> "ch_rank_B" [label="rank", color=green];
    "node_vertex_B" -> "ch_inbox_C" [label="to_C", color=green];
    "node_vertex_B" -> "ch_inbox_D" [label="to_D", color=green];
    "node_vertex_C" -> "ch_rank_C" [label="rank", color=green];
    "node_vertex_C" -> "ch_inbox_A" [label="to_A", color=green];
    "node_vertex_C" -> "ch_inbox_D" [label="to_D", color=green];
    "node_vertex_D" -> "ch_rank_D" [label="rank", color=green];
    "node_vertex_D" -> "ch_inbox_E" [label="to_E", color=green];
    "node_vertex_E" -> "ch_rank_E" [label="rank", color=green];
    "node_vertex_E" -> "ch_inbox_A" [label="to_A", color=green];
    "node_vertex_E" -> "ch_inbox_F" [label="to_F", color=green];
    "node_vertex_F" -> "ch_rank_F" [label="rank", color=green];
