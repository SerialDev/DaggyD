digraph PregelGraph {
    rankdir=LR;
    node [fontname="Arial"];
    edge [fontname="Arial"];
    labelloc="t";
    label="PageRank Graph";

    // Channels
    "ch_rank_A" [shape=ellipse, style=filled, fillcolor="lightblue", label="rank_A\n[LastValue]\n{'rank': 0.2555673827176243, '..."];
