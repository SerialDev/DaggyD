    "ch_rank_F" [shape=ellipse, style=filled, fillcolor="lightblue", label="rank_F\n[LastValue]\n{'rank': 0.09440334924051821, ..."];
    "ch_inbox_F" [shape=ellipse, style=filled, fillcolor="lightcoral", label="inbox_F\n[Accumulator]\n0.0"];

    // Nodes
    "node_vertex_A" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_A\n(succeeded)"];
    "node_vertex_B" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_B\n(succeeded)"];
    "node_vertex_C" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_C\n(succeeded)"];
    "node_vertex_D" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_D\n(succeeded)"];
    "node_vertex_E" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_E\n(succeeded)"];
    "node_vertex_F" [shape=box, style="filled,rounded", fillcolor="lightgreen", label="vertex_F\n(succeeded)"];

    // Subscriptions (channel -> node)
    "ch_rank_A" -> "node_vertex_A" [style=dashed, color=blue];
    "ch_inbox_A" -> "node_vertex_A" [style=dashed, color=blue];
    "ch_rank_B" -> "node_vertex_B" [style=dashed, color=blue];
    "ch_inbox_B" -> "node_vertex_B" [style=dashed, color=blue];
    "ch_rank_C" -> "node_vertex_C" [style=dashed, color=blue];
    "ch_inbox_C" -> "node_vertex_C" [style=dashed, color=blue];
    "ch_rank_D" -> "node_vertex_D" [style=dashed, color=blue];
    "ch_inbox_D" -> "node_vertex_D" [style=dashed, color=blue];
    "ch_rank_E" -> "node_vertex_E" [style=dashed, color=blue];
