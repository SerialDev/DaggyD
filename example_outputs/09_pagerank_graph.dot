    "ch_rank_C" [shape=ellipse, style=filled, fillcolor="lightblue", label="rank_C\n[LastValue]\n{'rank': 0.19040295900779777, ..."];
    "ch_inbox_C" [shape=ellipse, style=filled, fillcolor="lightcoral", label="inbox_C\n[Accumulator]\n0.0"];
    "ch_rank_D" [shape=ellipse, style=filled, fillcolor="lightblue", label="rank_D\n[LastValue]\n{'rank': 0.16270816861292095, ..."];
